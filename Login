class Login {
	
	
	@BeforeTestCase
	def logearse() {

	System.setProperty('webdriver.chrome.driver', DriverFactory.getChromeDriverPath())// Instanciar webdriver
  WebUI.openBrowser('url')//abrir el navegador
  WebDriver driver = DriverFactory.getWebDriver()

	driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS)

	driver.manage().window().maximize()
	
	driver.findElement(By.xpath('elemento ID').sendKeys('123456')// Ingresar numero id
  
  	driver.findElement(By.xpath('elemento pwd')).sendKeys('pwd')
	

	driver.findElement(By.xpath('elemento 1 checkbox')).click()

	driver.findElement(By.xpath('elemento 1 checkbox')).click()

	driver.findElement(By.xpath('elemento button')).click()
	
	WebUI.takeScreenshot()

	driver.findElement(By.id('Question')).sendKeys('Rta')

	WebUI.takeScreenshot()
	driver.findElement(By.xpath('elemento boton ')).click()
	
	Thread.sleep(10000)
	
	driver.findElement(By.xpath('boton')).click()
	
	
}
	
	
